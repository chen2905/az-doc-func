{
  "version": "2.0",
  "extensions": {
    "eventGrid": {
      "maxEventAgeInSeconds": 14400,
      "eventTimeToLiveInSeconds": 3600
    }
  },
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      },
      "enableLiveMetricsFilters": true
    }
  }
}
